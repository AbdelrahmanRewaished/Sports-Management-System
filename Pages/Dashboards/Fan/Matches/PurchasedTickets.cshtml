@page
@model Sports_Management_System.Pages.Dashboards.Fan.Matches.PurchasedTicketsModel
@{
    Layout = "~/Pages/Shared/_FanLayout.cshtml";

}

<div class="container row p-0 m-0">
    <div class="col-9">
        <h2 class="text-info">Purchased Tickets</h2>
    </div>
    <br />
    <br />
    <br />
    <div class="col-12 border p-3">
        <table id="TicketsTable" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Host Club</th>
                    <th>Guest Club</th>
                    <th>Start Time</th>
                    <th>Stadium</th>
                    <th>Location</th>
                    <th>No. of Buyed Tickets</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/fan/purchasedTickets.js"></script>
}